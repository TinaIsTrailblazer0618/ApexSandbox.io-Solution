public List<Integer> twoSum(List<Integer> nums, Integer target){
    //code here
    Integer leftBound = 0;
    Integer rightBound = nums.size() - 1;
    while (leftBound < rightBound) {
        Integer result = target - nums[leftBound];
        if (nums[rightBound] > result) {
            rightBound--;
        } else if (nums[rightBound] == result) {
            return new List<Integer>{ leftBound, rightBound};
        } else {
            leftBound++;
        }
    }
}