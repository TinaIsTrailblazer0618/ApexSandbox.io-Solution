public String divide(Integer a, Integer b){
    //code here
    Integer resultInt;
    String resultStr;
    if (b == 0) {
        return 'Divide by 0';
    }
    try {
        resultInt = a / b;
        resultStr = String.valueOf(resultInt);
        return resultStr;
    } catch (Exception error) {
        return error.getMessage();
    }
}