public static List<Integer> mergeLists(List<Integer> list1, List<Integer> list2){
    Integer i = 0;
    Integer j = 0;
    Integer k = 0;
    Integer listSize1 = list1.size();
    Integer listSize2 = list2.size();
    List<Integer> mergedList = new Integer[listSize1 + listSize2];

    while (i < listSize1 && j < listSize2) {
        if (list1[i] < list2[j]) {
            mergedList.add(k, list1[i]);
            i++;
            k++;
        } else {
            mergedList.add(k, list2[j]);
            j++;
            k++;
        }
    }
    while (i < listSize1) {
        mergedList.add(k++, list1[i++]);
    }
    while (j < listSize2) {
        mergedList.add(k++, list2[j++]);
    }

    return mergedList;
}